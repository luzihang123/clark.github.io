<script>
  var GUEST = ['nick', 'mail', 'link'];
  var guest = 'nick,mail,link';
  guest = guest.split(',').filter(function(item) {
    return GUEST.indexOf(item) > -1;
  });
  new Valine({
    el: '#comments',
    verify: false,
    notify: false,
    appId: 'nlS4qMp5lG9KCcIyQtO7QaQy-gzGzoHsz',
    appKey: 'YORuMopjXO5YVSu7AgrSvoGM',
